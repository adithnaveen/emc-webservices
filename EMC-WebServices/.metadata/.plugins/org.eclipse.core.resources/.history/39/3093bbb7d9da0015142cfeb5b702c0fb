package com.emc.entity;

public class Greeting {
	private String name;
	private String message;
	
	public Greeting(String name, String message){
		this.name = name;
		this.message = message;
	}

	@Override
	public String toString() {
		return "Greeting [name=" + name + ", message=" + message + "]";
	}
	
	
	
}
